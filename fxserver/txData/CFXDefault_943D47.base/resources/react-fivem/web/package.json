{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "watch": "cra-build-watch --disable-chunks --build-path ../resources/html --output-filename dev.js",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "build": "react-scripts build",
    "build:resources": "cross-env NODE_ENV=production webpack --config webpack/webpack.resources.js --color -p --progress --hide-modules --display-optimization-bailout",
    "test": "react-scripts test --watchAll=false --testMatch **/src/**/*.test.[jt]s?(x)",
    "test:watch": "react-scripts test --watchAll --testMatch **/src/**/*.test.[jt]s?(x)",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "7.14.2",
    "@material-ui/core": "4.10.2",
    "@material-ui/icons": "4.9.1",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "12.1.10",
    "color": "3.1.3",
    "d3": "6.7.0",
    "fivem-nui-react-lib": "2.2.2",
    "image-webpack-loader": "7.0.1",
    "jquery": "3.6.0",
    "jquery-ui-dist": "^1.12.1",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dnd": "14.0.2",
    "react-dnd-html5-backend": "14.0.0",
    "react-dom": "17.0.1",
    "react-hotkeys-hook": "3.3.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.0",
    "react-transition-group": "4.4.1",
    "recoil": "0.1.2",
    "storybook": "6.2.9",
    "web-vitals": "0.2.4"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/helper-builder-react-jsx": "7.12.13",
    "@babel/helper-builder-react-jsx-experimental": "7.12.11",
    "@babel/helper-define-map": "7.13.12",
    "@babel/helper-regex": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-react-jsx": "7.13.12",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-actions": "6.2.9",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/addon-links": "6.2.9",
    "@storybook/node-logger": "6.2.9",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.9",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "cra-build-watch": "3.2.0",
    "cross-env": "7.0.2",
    "css-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.3.0",
    "ifdef-loader": "2.1.5",
    "react-test-renderer": "16.13.1",
    "storybook-dark-mode": "1.0.8",
    "style-loader": "0.23.1",
    "svg-url-loader": "2.3.2",
    "terser-webpack-plugin": "1.3.0",
    "url-loader": "1.1.2",
    "webpack-cli": "3.3.12"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
